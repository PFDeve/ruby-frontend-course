<div class="container">
  <div class="page-header">
    <h1>Import</h1>
    <h4><span class="label label-primary">Import Covid Data</span></h4>
    <h4>
      <span class="label label-danger">
        Count: <%= @notes.count%>
      </span>
    </h4>
    <%= form_with(url: import_date_path, method: :post) do |f| %>
      <%= submit_tag("Import", class: "btn btn-primary") %>
    <% end %>
  </div>
</div>
<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Id</th>
        <th>Cases</th>
      </tr>
    </thead>
    <tbody>
      <% @notes.each do |note| %>
        <tr>
          <td><%= note.id %></td>
          <td><%= note.name %></td>
          <td> --- </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>